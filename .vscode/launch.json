{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pos App",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": ["--enable-software-rendering", "--no-enable-impeller"]
        },
        // buka emulator cmd : emulator -avd Medium_Phone_API_36 -gpu swiftshader_indirect
        //flutter run -d emulator-5554 --enable-software-rendering --no-enable-impeller
        {
            "name": "Run on Smartphone Emulator",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "emulator-5554"
        },
        // buka emulator cmd : emulator -avd Tab_10inch_-_API_36_Android_16 -gpu swiftshader_indirect
        //flutter run -d emulator-5556 --enable-software-rendering --no-enable-impeller
        {
            "name": "Run on Tab Emulator (10 inch)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "emulator-5556"
        },
        //flutter run -d R9RX8018G1V
        {
            "name": "Run on Tablet Real Device (7 inch)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "R9RX8018G1V"  // ID tablet hanif
        },
        //flutter run -d RR8TA05T5HW
        {
            "name": "Run on Samsung Galaxy A13",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "R9RX8018G1V"  // ID tablet hanif
        }
    ]
}
